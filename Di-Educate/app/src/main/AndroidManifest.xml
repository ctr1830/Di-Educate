        <activity android:name="eus.ehu.tta.intel.di_educate.Juego2" />
        <activity android:name="eus.ehu.tta.intel.di_educate.Escucho1" />
        <activity android:name="eus.ehu.tta.intel.di_educate.Escucho2" />
        <activity android:name="eus.ehu.tta.intel.di_educate.Escucho3" />
